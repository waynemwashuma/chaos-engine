{
    "plugins":[],
    "source":{
        "include":["src"],
        "includePattern":".js$",
        "excludePattern":"(node_modules/|docs)"
    },
    "templates":{
        "cleverLinks":true,
        "monospaace":true
    },
    "opts":{
        "destination":"./docs",
        "recurse":true,
        "encoding":"utf-8",
        "template":"docdash",
        "readme":"readme.md"
    },
    "docdash":{
        "search":"true",
        "static": false,
        "collapse": true,
        "typedefs": true
    }
}